    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "cwd": "d:\\LapTrinhUngDungWeb\\baitap3",
      "environment": [],
      "program": "d:\\LapTrinhUngDungWeb\\baitap3\\build\\Debug\\outDebug",
